<h1>Delete Project</h1>

<form action="/projects/delete" method="POST">
    <input type="hidden" name="_csrf" value="{{csrfToken}}">
    <div>
        Title: <input type="text" name="title" value=" {{title}}">
    </div>
    <div>
        Description: <input type="text" name="description" value="{{description}}">
    </div> 
    <div>
        Created Date: <input type="date" name="createdDate" value="{{createdDate}}">
    </div>
     <div>
        Last Updated Date: <input type="date" name="lastUpdatedDate" value="{{lastUpdatedDate}}">
    </div>
    <input type="submit" value="Delete Project">
</form>
